{
  "name": "@noyobo/style-factory",
  "version": "0.0.0",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "napi": {
    "name": "style-factory",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.10",
    "fs-extra": "^11.3.0",
    "style-factory": "^2.3.0",
    "tinybench": "^4.0.1",
    "vitest": "^3.0.9"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "bench": "vitest bench",
    "universal": "napi universal",
    "version": "napi version"
  }
}